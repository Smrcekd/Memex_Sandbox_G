@article{MaorReconciling2016,
  title = {Reconciling the {{Opposites}}: {{Max Brod}} and {{Nationalism}} in {{Prague}}},
  shorttitle = {Reconciling the {{Opposites}}},
  author = {Maor, Zohar},
  date = {2016-05},
  journaltitle = {German Studies Review},
  volume = {39},
  pages = {279--296,441},
  publisher = {{Johns Hopkins University Press}},
  location = {{Baltimore, United States}},
  issn = {01497952},
  url = {http://www.proquest.com/docview/1808336622/abstract/9B82A1756FA64038PQ/1},
  urldate = {2021-02-02},
  abstract = {[Martin Buber]'s Zionist creed had two contrasting messages. The explicit message was one of national and existential decisiveness. For him, the true misery of exile was the schism between the inner-the Jewish essence ("the blood") and the outer- the Jewish environment that de facto molds culture ("the world of impressions"). However, Buber's audiences in Prague could not overlook the implicit and inverse message of these speeches. Buber constructs "authentic" Jewish identity by utilizing the discourse of German antirational culture-of which Buber was a proponent even after joining the Zionists.30 Jewish "blood" can and should assimilate surrounding cultures. He stresses that the Jews "are a cultural admixture."31 Thus, the external could be metamorphosed and become internal. Indeed, Buber provided [Max Brod] and other Prague Zionists with a theoretical framework for affirming their distinctive Jewish identity while retaining their place in "the Bohemian mosaic" (Shumsky's term). Tycho is tortured by his own contradictions-his scientific indifferentism on the one hand and his political involvement and decisiveness, on the other. Both [Johannes Kepler] (whom Brod presents as Tycho's adopted son and heir) and Tengnagel, his son in law, are his alter egos.41 On one hand, Tycho shares Kepler's escapism; he proclaims that "life is so evil. . . . The best thing of all, therefore, is to inhabit an island. . . . With sea all about and walls too, no entrance" (36). Or, when clarifying Tycho's affection for symbols: "Assuredly one must oneself have been a storm tossed soul, the object of much hostility, to sympathetically appreciate so profound a seclusion of the spirit within itself" (35). One cannot ignore the famous insular image of Prague's Germans and its effect on the city's art;42 Brod alludes to this and adds the powerful image of the Jewish exile; he highlights Tycho's exilic "Jewish" existence in this passage and throughout the whole novel: "the Jewish People, homeless and fugitive like himself, the butt of perpetual hostility like himself . . . really seemed to him a veritable symbol of his own lot in life" (263). Unlike Kepler, however, Tycho is haunted by his exile. Until the revelation, the plot's climax, exile summons him to battle for his position. After the establishment of a Czechoslovak Republic, Brod found himself in a state whose existence he had opposed. In an essay written just before the end of the Great War and the final dissolution of the Habsburg Empire, Brod wrote: "My desired political aim is a fair federative regime of autonomous people in Austro-Hungary."45 This, as we well know, remained a dream. The Czechoslovak Republic was established as a democratic nation-state under Czech hegemony, with generous de jure minority rights to Germans, Hungarians, Ruthenians, Poles, and Jews; de facto, an ongoing struggle over rights and status was conducted throughout the turbulent years of the republic.46 Brod was an active player in this struggle, as vice president of the Jewish National Council, which represented the Jewish national cause to the Czech administration.47 Brod's stance regarding the complex interplay of nationalities in the new republic and his Zionism was that they were not two different issues, but rather two sides of the same coin. As a response to the Jews's pressing demand to reorient themselves after the dissolution of the empire, Brod presents a complex relationship to the various national causes and consequently, a very complex set of loyalties.48 The first, his Jewish "blood relationship" (Blutverwandschaft)49-a deep and partly unconscious relationship-is curbed and mediated by his second loyalty, a basic and mandatory humanism. The third is his relationship with the Germans, regarding German high culture and the fourth, his relationship with the Czechs concerning Czech popular-national culture.50 He promises to support any German or Czech campaign for safeguarding their rights to cultural well-being, but at the same time, to resist any aspiration for majoritarianism or exclusion of the other party.51 Brod is fully aware of this "Odradekian" (to use [Franz Kafka]'s parable52) identity: Indeed, the issues of the spiritual vs. the mundane, and consequently, the universal vs. the ethnic, concerned Brod politically and theologically, and his conclusions are not simple. In numerous publications, and especially in the ambitious treatise Heidentum, Christentum, Judentum, (Paganism, Christianity, Judaism) Brod tags völkisch ideology and prevailing variants of state nationalism as pagan and accuses them of instigating the catastrophe of the Great War.61 He also discredits "Christian" universalism; Brod sweepingly identifies enlightened humanism and its rebuff of power, ethnic differentiation and corporality, with Christian world-negation and spiritual indifferentism. For him, the paradoxical result of this negation is an ethical vacuum, inviting pagan powers to run wild, unhampered. This two-pronged debate is possibly partially related to another debate-against the intolerant and even violent aspects of Czech nationalism, on one hand, and against the universal discourse of some proponents of Jewish Czech assimilation, on the other.62 Judaism, for Brod, is the desired middle position-actually much closer to the maligned Christianity than he is willing to admit-between the two poles: a cautious world affirmation, including an affirmation of differences, which stems from worldly and not spiritual sources. The mundane not only realizes the spiritual but also bestows unique qualities. Instead of universalism, Brod suggests multinationalism: "Judaism believes that all differences can coexist with unity, and that difference is divinely ordained." Brod anchors this difference in deterministic racial divergence. He bases his Zionism and the Zionist demand for the right to identify as Jews in the Czechoslovakia censuses on the uniqueness of the Jews as a "pure" race, unlike the Germans and the Czech who have, in his view, the same racial origin, and on the impossibility of the Jews's national integration with Czechs or Germans.63 Thus, Brod's multinationalism is racially and not spiritually based.64},
  file = {C\:\\Users\\dsmrc\\Desktop\\ZoteroPDFs\\Maor_2016_Reconciling the Opposites.pdf},
  keywords = {Art,Aspiration,Complexity,Contemporary problems,Cultural identity,Culture,Czech,Ethics,German language,Hands,History--History Of Europe,Ideology,Jews,Language culture relationship,Literature,Minority & ethnic groups,Nationalism,Negation,Political Science,Politics,Studies,War},
